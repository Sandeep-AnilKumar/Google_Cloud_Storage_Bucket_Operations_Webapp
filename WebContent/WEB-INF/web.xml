<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>BucketOperations</display-name>
  <welcome-file-list>
  	<welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>bucketoperations</servlet-name>
  	  <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
      <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>bucketoperations</servlet-name>
    <url-pattern>/bucketlist.jsp</url-pattern>
    <url-pattern>/bucketlist-form.jsp</url-pattern>
    <url-pattern>/delete.jsp</url-pattern>
    <url-pattern>/download-item.jsp</url-pattern>
    <url-pattern>/upload-file.jsp</url-pattern>
    <url-pattern>/upload-normal-file.jsp</url-pattern>
  </servlet-mapping>
</web-app>